{
    "version": "The version string is optional. It may have (future?) meaning in CSV mode.",
    "description": "Redfish doc generator example: Configuration file for CSV mode.",
    "format": "csv",
    "import_from": [ "./json-schema" ],
    "outfile": "csv.csv",
    "uri_mapping": { "redfish.dmtf.org/schemas/v1": "./json-schema" },
    "excluded_properties": [
        "@odata.context",
        "@odata.type",
        "@odata.id",
        "@odata.etag",
        "Name",
        "Id",
        "Description",
        "Oem"
    ],
    "excluded_annotations": [
        "*@odata.count",
        "*@odata.navigationLink"
    ],
    "excluded_schemas": [
        "*Collection",
        "HostedStorageServices",
        "StorageService",
        "StorageSystem",
        "idRef",
        "Oem"
    ],
    "excluded_pattern_properties": [
        "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message)\\.[a-zA-Z_][a-zA-Z0-9_]*$"
    ]
}
