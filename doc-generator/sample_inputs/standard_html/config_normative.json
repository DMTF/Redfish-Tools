{
    "version": "The version string is optional. It may have (future?) meaning in standard normative mode in HTML format.",
    "description": "Redfish doc generator example: Configuration file for standard normative mode in HTML format.",
    "uri_mapping": { "redfish.dmtf.org/schemas/v1": "./json-schema" },
    "format": "html",
    "import_from": [ "./json-schema" ],
    "outfile": "normative.html",
    "normative": true,
    "payload_dir": "./mockups/DSP2046-examples",
    "add_toc": true,
    "html_title": "Sample Standard Output",
    "combine_multiple_refs": 3,
    "excluded_properties": [
        "@odata.context",
        "@odata.type",
        "@odata.id",
        "@odata.etag",
        "Name",
        "Id",
        "Description",
        "Oem"
    ],
    "excluded_annotations": [
        "*@odata.count",
        "*@odata.navigationLink"
    ],
    "excluded_schemas": [
        "*Collection",
        "HostedStorageServices",
        "StorageService",
        "StorageSystem",
        "idRef",
        "Oem"
    ],
    "excluded_pattern_properties": [
        "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message)\\.[a-zA-Z_][a-zA-Z0-9_]*$"
    ],
    "object_reference_disposition": {
        "common_object": [
            "Redundancy"
        ],
        "include": [
            "http://redfish.dmtf.org/schemas/v1/PCIeDevice.json#/definitions/PCIeInterface"
        ]
    },
    "boilerplate_intro": "./intro.md",
    "boilerplate_postscript": "./postscript.md",
    "content_supplement": "./content_supplement.json"
}
